{
  "compilerOptions": {
    "jsx": "react",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@pages/*": ["src/pages/*"],
      "@styling/*": ["src/styling/*"],
      "@components/*": ["src/components/*"],
      "@ui/*": ["src/components/ui/*"],
      "@src/*": ["src/*"]
    },
    "esModuleInterop": true
  },
  "extends": "expo/tsconfig.base"
}
